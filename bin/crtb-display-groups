#!/usr/bin/env raku
use v6.d;

use App::Classroom::Toolbox;

sub MAIN(Str:D :g(:$grouping-file)? where *.IO.f = %*ENV<CRTB_GROUP>, #= file containing grouping; --class-file=groups/demo-grouping or 'export CRTB_GROUP=groups/demo-grouping'
         ) {

    print "\n";
    .join("\t").say for read-group-file($grouping-file);
    print "\n";

}
